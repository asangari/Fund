6.  &  \ \ \ \ensuremath{\forall x\exists y\left(x+y=0\right)} &  AG3 \\
7.  &  \ \ \ \ensuremath{\exists y\left(x+y=0\right)} &  spec \\
8.  &  \ \ \ \,\,\,\,\ensuremath{\left(x+y=0\right)} &  C\mbox{\ensuremath{y}} \\
9.  &  \ensuremath{\ \ \ \,\,\,\,\left(z+x\right)+y=\left(z_{1}+x\right)+y} &  sub,5 \\
10.  &  \ \ \ \ensuremath{\,\,\,\,\forall x\forall y\forall z\left(\left(x+y\right)+z=x+\left(y+z\right)\right)} &  AG1\\
11.  &  \,\,\,\,\,\,\,\,\,\ensuremath{\left(z+x\right)+y=z+\left(x+y\right)} &  spec\\
12.  &  \ensuremath{\,\,\,\,\,\,\,\,\,\ensuremath{\left(z_{1}+x\right)+y=z_{1}+\left(x+y\right)}} &  spec,10\\
13.  &  \,\,\,\,\,\,\,\,\,\,\mbox{\ensuremath{z+\left(x+y\right)=z_{1}+\left(x+y\right)}}  &  =,11,9,12\\
