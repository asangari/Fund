\begin_body

\begin_layout Section*
Esquema de teoremas
\end_layout

\begin_layout Standard
Abajo mostramos alguno de los esquemas de teoremas más importantes del cálculo
 de predicado.
 (Trate de hacer una demostración de todos los teoremas)
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\forall vP\rightarrow P\left(t/v\right)$
\end_inset

 si 
\begin_inset Formula $P$
\end_inset

 admite a 
\begin_inset Formula $t$
\end_inset

 para 
\begin_inset Formula $v$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\forall vP\rightarrow\exists vP$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\forall vP\leftrightarrow P$
\end_inset

 si 
\begin_inset Formula $v$
\end_inset

 no es libre en 
\begin_inset Formula $P$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\forall uP\left(u\right)\leftrightarrow\forall vP\left(v\right)$
\end_inset

 si 
\begin_inset Formula $P\left(u\right)$
\end_inset

 es similar a 
\begin_inset Formula $P\left(v\right)$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\forall u\forall vP\leftrightarrow\forall v\forall uP$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\forall vP\leftrightarrow\sim\exists v\sim P$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\forall v\sim P\leftrightarrow\sim\exists vP$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $P\left(t/v\right)\rightarrow\exists vP$
\end_inset

 si 
\begin_inset Formula $P$
\end_inset

 admite 
\begin_inset Formula $t$
\end_inset

 para 
\begin_inset Formula $v$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\exists vP\leftrightarrow P$
\end_inset

 si 
\begin_inset Formula $v$
\end_inset

 no es libre en 
\begin_inset Formula $P$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\exists uP\left(u\right)\leftrightarrow\exists vP\left(v\right)$
\end_inset

 si 
\begin_inset Formula $P\left(u\right)$
\end_inset

 es similar a 
\begin_inset Formula $P\left(v\right)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\exists u\exists vP\leftrightarrow\exists v\exists uP$
\end_inset


\end_layout

\begin_layout Enumerate
